# Configuração de teste
NODE_ENV=test

# Database
DATABASE_URL="file:./prisma/test.db"

# JWT
JWT_SECRET=test-jwt-secret-key
JWT_EXPIRES_IN=1h
JWT_REFRESH_SECRET=test-refresh-secret-key
JWT_REFRESH_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3000
CORS_CREDENTIALS=true

# Proteção do Banco de Dados - Permitir limpeza em testes
DATABASE_PROTECTION_ENABLED=true
ALLOW_DATABASE_CLEANUP=true
DATABASE_OPERATION_TOKEN=test-admin-token-2024

# Rate Limiting - Mais permissivo para testes
THROTTLE_TTL=60
THROTTLE_LIMIT=1000

# Cache
CACHE_TTL=60
CACHE_MAX=50