{
  "name": "cyberlens",
  "version": "1.0.0",
  "description": "Aplicativo de Monitoramento de Redes Wi-Fi",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:mobile\"",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:mobile": "cd mobile && npm start",
    "build": "npm run build:backend && npm run build:mobile",
    "build:backend": "cd backend && npm run build",
    "build:mobile": "cd mobile && npm run build",
    "test": "npm run test:backend && npm run test:mobile",
    "test:backend": "cd backend && npm run test",
    "test:mobile": "cd mobile && npm test",
    "lint": "npm run lint:backend && npm run lint:mobile",
    "lint:backend": "cd backend && npm run lint",
    "lint:mobile": "cd mobile && npm run lint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:generate": "cd backend && npx prisma generate",
    "db:studio": "cd backend && npx prisma studio",
    "db:seed": "cd backend && npx prisma db seed",
    "setup": "npm install && npm run setup:backend && npm run setup:mobile",
    "setup:backend": "cd backend && npm install",
    "setup:mobile": "cd mobile && npm install"
  },
  "keywords": [
    "wifi",
    "network",
    "monitoring",
    "firewall",
    "react-native",
    "nestjs",
    "prisma",
    "postgresql"
  ],
  "author": "CyberLens Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "workspaces": [
    "backend",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberlens/cyberlens-1.0.git"
  },
  "bugs": {
    "url": "https://github.com/cyberlens/cyberlens-1.0/issues"
  },
  "homepage": "https://github.com/cyberlens/cyberlens-1.0#readme"
}